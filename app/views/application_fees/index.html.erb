<p id="notice"><%= notice %></p>

<h1>Listing Application Fees</h1>

<table class='table'>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @application_fees.each do |application_fee| %>
      <tr>
        <td><%= link_to 'Show', application_fee %></td>
        <td><%= link_to 'Edit', edit_application_fee_path(application_fee) %></td>
        <td><%= link_to 'Destroy', application_fee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Application fee', new_application_fee_path %>
