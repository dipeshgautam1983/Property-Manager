<p id="notice"><%= notice %></p>

<h1>Listing Agents</h1>

<table class='table'>
  <thead>
    <tr>
      <th>Name</th>
      <th>ID#</th>
      <th>Address</th>
      <th>Contact</th>
      <th>Email</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agents.each do |agent| %>
      <tr>
        <td><%= agent %></td>
        <td><%= agent.identification %></td>
        <td><%= agent.address %></td>
        <td><%= agent.contact %></td>
        <td><%= agent.email %></td>
        <td><%= agent.type %></td>
        <td><%= link_to 'Show', agent %></td>
        <td><%= link_to 'Edit', edit_agent_path(agent) %></td>
        <td><%= link_to 'Destroy', agent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agent', new_agent_path %>
